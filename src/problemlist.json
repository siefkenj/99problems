    "1": {
        "description": "Find the last element of a list.",
        "difficulty": 1,
        "function-name": "last",
	"category": "Lists",
	"abstract-example": "\\( (x_1,x_2,\\ldots,x_n)\\mapsto x_n \\)",
        "language-overrides": {
		"haskell": {
			"examples": [
				{"input": "FN [1..10]", "output": "10"},
				{"input": "FN \"abc\"", "output": "c"}
			]
		}
	}
    },
    "2": {
